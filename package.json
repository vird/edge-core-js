{
  "name": "edge-core-js",
  "version": "0.16.13",
  "description": "Edge account & wallet management library",
  "keywords": [
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "homepage": "https://edge.app",
  "repository": {
    "type": "git",
    "url": "git@github.com:EdgeApp/edge-core-js.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "William Swanson <william@edge.app>",
    "Tim Horton <tim@edge.app>",
    "Paul Puey <paul@edge.app>",
    "Eliran Zach <eliran@edge.app>",
    "Kevin Sullivan <kevin@edge.app>",
    "Allen Hujsak <allen@edge.app>"
  ],
  "files": [
    "/CHANGELOG.md",
    "/index.js.flow",
    "/index.js",
    "/lib/*",
    "/LICENSE",
    "/package.json",
    "/README.md",
    "/src/types/*",
    "/types.js.flow",
    "/types.js"
  ],
  "main": "./index.js",
  "module": "./lib/index.js",
  "browser": "./lib/browser.js",
  "scripts": {
    "build": "rimraf lib && sucrase ./src -q -d ./lib -t flow,jsx && node ./make-types.js && webpack",
    "flow": "flow",
    "format": "npm run lint -- --fix",
    "lint": "eslint .",
    "lint:fix": "npm run format",
    "precommit": "lint-staged && flow && npm test && npm run build",
    "prepare": "npm run build",
    "test": "mocha -r sucrase/register 'test/**/*.test.js'",
    "updot": "updot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {
    "aes-js": "^3.1.0",
    "base-x": "^1.0.4",
    "biggystring": "^3.0.2",
    "currency-codes": "^1.1.2",
    "disklet": "https://github.com/vird/disklet#master",
    "elliptic": "^6.4.0",
    "esm": "^3.2.4",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "hash.js": "^1.0.3",
    "hmac-drbg": "^1.0.1",
    "json-csv": "^1.5.0",
    "node-fetch": "^2.0.0",
    "redux": "^3.6.0",
    "redux-keto": "^0.3.2",
    "redux-pixies": "^0.3.6",
    "rfc4648": "^1.1.0",
    "scrypt-js": "^2.0.3",
    "utf8": "^3.0.0",
    "ws": "^5.1.1",
    "yaob": "^0.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "eslint": "6.1.0",
    "eslint-config-standard-kit": "^0.14.4",
    "eslint-plugin-flowtype": "^3.12.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.89.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.1.0",
    "mocha": "^6.2.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.2",
    "sucrase": "^3.9.5",
    "updot": "^1.1.7",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "react-native": "lib/react-native.js"
}
